{
  "main": "dist/app.js",
  "scripts": {
  "build": "tsc && node copy-ejs.js",
  "dev": "ts-node src/app.ts",
  "start": "node dist/app.js"
},
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "@quixo3/prisma-session-store": "3.1.13",
    "bcrypt": "^5.1.1",
    "bootstrap-icons": "^1.13.1",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1",
    "express-session": "1.18.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.12.0",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "uuid": "11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/express-session": "1.18.1",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.7",
    "@types/passport": "1.0.17",
    "@types/passport-local": "1.0.38",
    "nodemon": "3.1.9",
    "prisma": "6.3.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3"
  }
}



